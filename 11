var app = angular.module('myApp' , [] );
app.controller('FirstController' , function($log, $scope ,$timeout ){
     window.addEventListener('mouseover' , function(){

        if(window.location.hash==='#bookmark')
        {
           console.log("Page mast hai");
        }
     });

});



